{
    "name": "fawkescreatives/myanmar-phone",
    "description": "This package is for building easy to do Myanmar phone number in laravel.",
    "keywords": [
        "laravel",
        "myanmar",
        "phone",
        "phone-number",
        "myanmar-phone",
        "myanmar-phone-number",
        "mpt",
        "telenor",
        "ooredoo"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fawkes Creatives",
            "email": "fawkescreatives@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "giggsey/libphonenumber-for-php": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "nunomaduro/collision": "^7.0|^8.1",
        "larastan/larastan": "^2.0|^3.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/extension-installer": "^1.1|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1|^2.0",
        "phpstan/phpstan-phpunit": "^1.1|^2.0"
    },
    "autoload": {
        "psr-4": {
            "MyanmarPhone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyanmarPhone\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyanmarPhone\\MyanmarPhoneServiceProvider"
            ],
            "aliases": {
                "MyanmarPhone": "MyanPhone"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
