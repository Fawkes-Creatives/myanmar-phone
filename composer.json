{
    "name": "fawkescreatives/myanmar-phone",
    "description": "This package is for building easy to do Myanmar phone number in laravel.",
    "keywords": [
        "laravel",
        "myanmar",
        "phone",
        "phone-number",
        "myanmar-phone",
        "myanmar-phone-number",
        "mpt",
        "telenor",
        "ooredoo"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fawkes Creatives",
            "email": "fawkescreatives@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "giggsey/libphonenumber-for-php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MyanmarPhone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyanmarPhone\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyanmarPhone\\MyanmarPhoneServiceProvider"
            ],
            "aliases": {
                "MyanmarPhone": "MyanPhone"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
